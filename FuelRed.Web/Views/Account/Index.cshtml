@model IEnumerable<FuelRed.Web.Models.UserViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Permisions Manager</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Station)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.UserType)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Station)
    </td>

    <td>
        @Html.ActionLink("Change Type", "Roles", new { email = item.Email })

    </td>
</tr>
    }

</table>


